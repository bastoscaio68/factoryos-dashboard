{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/projetos",
      "dest": "/api/projetos.js"
    },
    {
      "src": "/api/arquivos",
      "dest": "/api/arquivos.js"
    }
  ]
}
